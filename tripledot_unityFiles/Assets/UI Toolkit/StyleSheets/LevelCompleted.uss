    padding-right: 60px;
    padding-bottom: 60px;
    padding-left: 60px;
    margin-top: 30px;
    margin-right: 0;
    margin-bottom: 30px;
    margin-left: 0;
    flex-shrink: 1;
    flex-grow: 1;
    flex-direction: row;
    justify-content: center;
    align-items: stretch;
    align-content: auto;
    background-size: contain;
    background-position-x: center 0;
    -unity-slice-left: 0;
    -unity-slice-top: 0;
    -unity-slice-right: 0;
    -unity-slice-bottom: 0;
    -unity-slice-type: sliced;
    width: 300px;
    height: 300px;
    position: relative;
}

