:root {
}

.RewardIcon {
    -unity-background-scale-mode: scale-to-fit;
    background-size: contain;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    margin-top: 40px;
    margin-right: 40px;
    margin-bottom: 60px;
    margin-left: 40px;
    width: 220px;
    background-position-y: center 0;
    align-self: flex-start;
    height: 230px;
}

.RewardValue {
    font-size: 90px;
    color: rgb(255, 255, 255);
    -unity-text-outline-width: 3px;
    -unity-text-outline-color: rgb(25, 25, 112);
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    bottom: -180px;
    -unity-font-definition: url("project://database/Assets/resources/fonts/Lato-Black.ttf?fileID=12800000&guid=5b0caedb56d1b774ea9d6c8f69fe1c54&type=3#Lato-Black");
    text-shadow: 5px 10px 5px rgba(0, 0, 0, 0.39);
    flex-grow: 1;
    align-self: stretch;
    align-items: stretch;
    justify-content: flex-start;
}

.homeButton {
    background-color: rgba(188, 188, 188, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
