    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgba(0, 0, 0, 0);
    position: absolute;
    top: auto;
    left: 0;
    right: 0;
    bottom: 100px;
    -unity-font-definition: url("project://database/Assets/resources/fonts/Lato-Black.ttf?fileID=12800000&guid=5b0caedb56d1b774ea9d6c8f69fe1c54&type=3#Lato-Black");
    -unity-text-align: upper-center;
    -unity-text-outline-width: 10px;
    -unity-text-outline-color: rgba(0, 0, 0, 0.39);
    text-shadow: 5px 10px 5px rgba(0, 0, 0, 0.39);
    letter-spacing: -6px;
    -unity-font-style: bold;
}

.settingsButton {
    width: 500px;
    height: 200px;
    background-image: url("project://database/Assets/UI/_Rectangle-4-copy.png?fileID=-1500791171311278953&guid=5c8d7de59cc92064da5d1d28fda4c7c1&type=3#_Rectangle-4-copy_0");
