    background-image: url("project://database/Assets/UI/_divider.png?fileID=2725667923175964883&guid=06d8955ebe46b9a47ae1a1613dca5f06&type=3#_divider_0");
    -unity-background-scale-mode: stretch-to-fill;
}

.footer {
    position: absolute;
    bottom: 0;
    height: 400px;
    width: 100%;
    flex-direction: column;
    justify-content: flex-end;
    background-image: url("project://database/Assets/UI/_Rectangle-2-copy-4.png?fileID=705081714262086179&guid=c7381f69ee542084a994873498169729&type=3#_Rectangle-2-copy-4_0");
    -unity-background-scale-mode: stretch-to-fill;
    top: auto;
    left: 0;
    right: 0;
}

.supportButton {
