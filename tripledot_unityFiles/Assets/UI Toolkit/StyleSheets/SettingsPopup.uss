    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    margin-left: 50px;
    margin-right: 50px;
    flex-direction: row;
    -unity-font-definition: url("project://database/Assets/resources/fonts/Lato-Black.ttf?fileID=12800000&guid=5b0caedb56d1b774ea9d6c8f69fe1c54&type=3#Lato-Black");
    -unity-text-outline-width: 8px;
    -unity-text-outline-color: rgba(0, 0, 0, 0.39);
    text-shadow: 5px 10px 5px rgba(0, 0, 0, 0.39);
    -unity-text-align: middle-center;
}

.settingsButton:hover {
    margin: 0 5px;
    -unity-background-image-tint-color: rgb(199, 199, 199);
    margin-right: 50px;
    margin-left: 50px;
}

.versionLabel {
    font-size: 60px;
    color: rgb(25, 25, 112);
    -unity-font-definition: url("project://database/Assets/resources/fonts/Lato-Black.ttf?fileID=12800000&guid=5b0caedb56d1b774ea9d6c8f69fe1c54&type=3#Lato-Black");
}

.closeButton {
    position: absolute;
    top: 50px;
    right: 50px;
    width: 100px;
    height: 100px;
    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-image: url("project://database/Assets/UI/_X.png?fileID=2800000&guid=f144d71936f8a8d429ba33a08f221c55&type=3#_X");
    -unity-background-scale-mode: scale-to-fit;
}

.footerBottom {
    position: absolute;
    top: auto;
    left: 0;
    right: 0;
    bottom: 100px;
    flex-direction: row;
    align-items: center;
    justify-content: space-evenly;
    margin-left: 90px;
    margin-right: 90px;
}

.supportButton:hover {
    -unity-background-image-tint-color: rgb(127, 255, 0);
}
